{
	"trailingComma": "none",
	"tabWidth": 3,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParents": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/routes/",
		"/middleware/",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
